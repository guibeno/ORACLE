DECLARE -- criacao do bloco
    numero NUMBER := 3; -- define o numero que busco na lista
BEGIN
    IF numero IN (1,2,3,4,5) THEN  -- logica consiste aqui, se o numero for encontrado na lista , numero encontrado, se não cai para o else e emite a mensagem de erro
        DBMS_OUTPUT.PUT_LINE('numero '||numero||' encontrado na lista');
    ELSE
        DBMS_OUTPUT.PUT_LINE('numero não encontrado na lista'); -- mensagem de erro
    END IF;
END;

CREATE OR REPLACE PROCEDURE retorna_numeros_quadrado(numero IN NUMBER) IS -- criei a procedure que recebe um number como parametro, este numero será o total de numeros aleatorios que irei gerar
    numero_aleatorio NUMBER;
    numero_ao_quadrado NUMBER;
BEGIN
    FOR i IN 1 .. numero LOOP
        numero_aleatorio := TRUNC(DBMS_RANDOM.VALUE(1,100));
        numero_ao_quadrado := numero_aleatorio * numero_aleatorio;
        DBMS_OUTPUT.PUT_LINE('Número analisado: ' || numero_aleatorio || ', Número ao quadrado: ' || numero_ao_quadrado);
    END LOOP;
END;



BEGIN
    retorna_numeros_quadrado(5);
END;

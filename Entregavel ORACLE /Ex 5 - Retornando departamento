DECLARE
    id_do_departamento NUMBER := 10;

    CURSOR lista_funcionarios IS
        SELECT e.employeename 
        FROM employees_nopk e
        WHERE e.dptoid = id_do_departamento; 

BEGIN
    FOR i IN lista_funcionarios LOOP
        DBMS_OUTPUT.PUT_LINE('Nome do funcion√°rio: ' || i.employeename);
    END LOOP;
END;
